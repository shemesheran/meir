&lt;script&gt;
var AllImg = [];
var Aimg ="";
var data= new FormData();

$(document).ready(function() {
$('[id^=img_]').each(function( i, val )
{
	Aimg+=val.id.replace('img_','')+",";
	data.append(val.id.replace('img_',''),val.id.replace('img_',''));
	//AllImg[i]=val.id.replace('img_','');
	//console.log(val.id.replace('img_',''));

});


            var xhr = new XMLHttpRequest();
            xhr.open('GET', 'ajax_img.asp?Idx='+ Aimg);
            xhr.send(data);
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 &amp;&amp; xhr.status == 200) {
			console.log(xhr.responseText);
			$(xhr.responseText).each(function (i, val) {
				$('#'+ val.id).attr("src",$(val).attr('src'));
			});
                    //SaveMessage('success', 'נשמר בהצלחה', 'check')
                }
                else {
                    //SaveMessage('danger', 'אירעה שגיאה', 'warning')
                }
		};

});
</script>

